<h1>Create a new restaurant</h1>


<%= form_for(@restaurant) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :address %>
    <%= f.text_field :address %>
    <%= f.label :rating %>
    <%= f.text_field :rating %>
    <%= f.submit %>
<% end %>

<%= link_to 'back to main page', restaurants_path %>